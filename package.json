    "clean": "rimraf dist output",
    "packing": "ncc build dist/main.js -o output",
    "package": "run-z clean compile packing"
  },
  "devDependencies": {
    "@book000/eslint-config": "1.8.62",
    "@book000/node-utils": "1.15.1",
    "@types/node": "22.15.3",
    "@vercel/ncc": "0.38.3",
    "discord.js": "14.19.2",
    "eslint": "9.25.1",
    "eslint-config-standard": "17.1.0",
