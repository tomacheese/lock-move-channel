    "eslint-plugin-n": "17.23.1",
    "eslint-plugin-promise": "7.2.1",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "run-z": "2.1.0",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "typescript-json-schema": "0.65.1"
