    "packing": "ncc build dist/main.js -o output",
    "package": "run-z clean compile packing"
  },
  "devDependencies": {
    "@book000/eslint-config": "1.12.13",
    "@book000/node-utils": "1.18.174",
    "@types/node": "24.10.3",
    "@vercel/ncc": "0.38.4",
    "discord.js": "14.25.1",
    "eslint": "9.39.2",
    "eslint-config-standard": "17.1.0",
