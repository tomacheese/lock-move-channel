    "eslint-plugin-n": "17.17.0",
    "eslint-plugin-promise": "7.2.1",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
    "run-z": "2.1.0",
    "tsx": "4.19.3",
    "typescript": "5.8.3",
    "typescript-json-schema": "0.65.1"
