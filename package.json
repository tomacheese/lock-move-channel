    "clean": "rimraf dist output",
    "packing": "ncc build dist/main.js -o output",
    "package": "run-z clean compile packing"
  },
  "devDependencies": {
    "@book000/eslint-config": "1.11.13",
    "@book000/node-utils": "1.18.147",
    "@types/node": "24.9.1",
    "@vercel/ncc": "0.38.4",
    "discord.js": "14.24.0",
    "eslint": "9.38.0",
    "eslint-config-standard": "17.1.0",
